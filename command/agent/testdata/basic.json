            {
              "foo": "bar",
              "nested": [
                {
                  "bam": 2
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "exec": [
        {
          "config": [
            {
              "foo": true
            }
          ]
        }
      ]
    }
  ],
  "plugin_dir": "/tmp/nomad-plugins",
  "ports": [
    {
      "http": 1234,
      "rpc": 2345,
      "serf": 3456
    }
  ],
  "region": "foobar",
  "sentinel": [
    {
      "import": [
        {
          "foo": [
            {
              "args": [
                "a",
                "b",
                "c"
              ],
              "path": "foo"
            }
          ]
        },
        {
          "bar": [
            {
              "args": [
                "x",
                "y",
                "z"
              ],
              "path": "bar"
            }
          ]
        }
      ]
    }
  ],
  "server": [
    {
      "authoritative_region": "foobar",
      "bootstrap_expect": 5,
      "csi_plugin_gc_threshold": "12h",
      "csi_volume_claim_gc_threshold": "12h",
      "data_dir": "/tmp/data",
      "deployment_gc_threshold": "12h",
