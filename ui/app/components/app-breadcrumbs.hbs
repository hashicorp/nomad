<Breadcrumbs as |breadcrumbs|>
  {{#each breadcrumbs as |crumb iter|}}
    {{#let crumb.args.crumb as |c|}}
      {{component (concat "breadcrumbs/" (or c.type "default")) crumb=c isOneCrumbUp=(action this.isOneCrumbUp iter breadcrumbs.length)}}
    {{/let}}
  {{/each}}
</Breadcrumbs>