<LineChart
  @data={{@data}}
  @xProp="timestamp"
  @yProp="percent"
  @chartClass={{@chartClass}}
  @timeseries={{true}}
  @title="Stats Time Series Chart"
  @description={{this.description}}
  @xScale={{this.xScale}}
  @yScale={{this.yScale}}
  @xFormat={{this.xFormat}}
  @yFormat={{this.yFormat}}>
  <:svg as |c|>
    <c.Area @data={{@data}} @colorClass={{@chartClass}} />
  </:svg>
</LineChart>
