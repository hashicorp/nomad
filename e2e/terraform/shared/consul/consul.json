{
  "log_level": "INFO",
  "server": true,
  "ui": true,
  "data_dir": "/opt/consul/data",
  "bind_addr": "0.0.0.0",
  "client_addr": "0.0.0.0",
  "advertise_addr": "{{ GetPrivateIP }}",
  "bootstrap_expect": SERVER_COUNT,
  "service": {
    "name": "consul"
  },
  "retry_join": ["RETRY_JOIN"],
  "connect": {
    "enabled": true
  },
  "ports": {
    "grpc": 8502
  }
}
